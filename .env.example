# Voice-to-Airtable Environment Configuration
# Copy this file to .env and fill in your values

# Anthropic API Key (required) - for Claude field extraction
ANTHROPIC_API_KEY=sk-ant-...

# OpenAI API Key (required) - for Whisper transcription
OPENAI_API_KEY=sk-proj-...

# Airtable Configuration (required)
AIRTABLE_API_KEY=pat...
EF_SANJUAN_CRM_BASE_ID=appXXXXXXXXXXXXXX
LEADS_TABLE_ID=tblXXXXXXXXXXXXXX

# Server Configuration (Railway sets PORT automatically)
HOST=0.0.0.0
PORT=8000
DEBUG=false
